{
    "name": "tarpaulin",
    "version": "1.2.0",
    "description": "A minimal toolset aimed at mathematical drawing on canvas or svg",
    "main": "./lib/index.js",
    "scripts": {
        "build": "cross-env BABEL_ENV=production babel src --out-dir lib",
        "clean": "rimraf lib; rimraf dist",
        "lint": "eslint . --fix",
        "prepare": "npm run clean && npm run lint && npm run test && npm run build",
        "test": "echo \"No tests implemented yet\" && exit 0"
    },
    "files": [
        "lib",
        "src"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mattor/tarpaulin.git"
    },
    "keywords": [
        "browser",
        "canvas",
        "draw",
        "graphics",
        "javascript",
        "maths",
        "svg"
    ],
    "author": "Torbj√∏rn Mathisen <herrmathisen@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/mattor/tarpaulin/issues"
    },
    "homepage": "https://github.com/mattor/tarpaulin#readme",
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.3.4",
        "@babel/polyfill": "^7.2.5",
        "@babel/preset-env": "^7.3.4",
        "babel-eslint": "^10.0.1",
        "babel-preset-minify": "^0.5.0",
        "cross-env": "^5.2.0",
        "eslint": "^5.15.2",
        "rimraf": "^2.6.3"
